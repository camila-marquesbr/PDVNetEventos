<Window x:Class="PDVNetEventos.Views.ListarFornecedores"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Fornecedores" Height="420" Width="640"
        WindowStartupLocation="CenterScreen">
    <DockPanel>
        <DataGrid ItemsSource="{Binding Itens}" AutoGenerateColumns="True"
                  DockPanel.Dock="Top" Margin="10"/>
    </DockPanel>
</Window>